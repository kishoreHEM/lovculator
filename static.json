{
  "root": "./",
  "clean_urls": false,
  "routes": {
    "/**": "index.html"
  },
  "redirects": {
    "/": "/index.html",
    "/home": "/index.html",
    "/about": "/about.html",
    "/contact": "/contact.html",
    "/privacy": "/privacy.html", 
    "/terms": "/terms.html",
    "/record": "/record.html"
  },
  "headers": {
    "/*.html": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    },
    "/css/*": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/js/*": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/images/*": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/sw.js": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    },
    "/manifest.json": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    }
  }
}